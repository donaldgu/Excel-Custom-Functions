{
  "name": "excel-custom-functions",
  "version": "2.0.0",
  "main": "./src/customfunctions.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "start npm run dev-server && start npm run watch && npm run sideload",
    "start-web": "start npm run dev-server && start npm run watch",
    "build": "webpack -p --mode production",
    "dev-server": "webpack-dev-server --mode development --https --key ./certs/server.key --cert ./certs/server.crt --cacert ./certs/ca.crt --port 8081 --hotOnly",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "watch": "webpack -p --mode development --watch",
    "generate-json": "node ./src/createfunctionsjson.js ./src/customfunctions.ts",
    "generate-json-filepath": "node ./src/createfunctionsjson.js"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "office-addin-node-debugger": "^0.1.0",
    "office-toolbox": "^0.1.0",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "fs": "0.0.1-security"
  }
}
